@import url(http://fonts.googleapis.com/css?family=Yanone+Kaffeesatz);

body
	background-color #cccbc8
	font-family: 'Yanone Kaffeesatz', sans-serif;
	font-weight normal
	font-style normal

#container
	width 100%
#acordeon
	background-color #056a63
	width 250px
	margin 100px auto 0 auto 
	color white
	box-shadow 0 5px 15px 1px rgba(0, 0, 0, 0.6)
	box-shadow 0 0 200px 1px rgba(255, 255, 255, 0.5)	
	-webkit-box-shadow 0 5px 15px 1px rgba(0, 0, 0, 0.6)
	-webkit-box-shadow 0 0 200px 1px rgba(255, 255, 255, 0.5)
#acordeon h3
	font-size 12px
	line-height 34px
	padding 0 10px
	margin 0
	/*fondo gradiente*/
	background-color #02382d
	background linear-gradient(#005041,#022211)
#acordeon h3 span 
	font-size 16px
	margin-right 10px 
#acordeon ul
	margin 0px
	padding 0px 
#acordeon li 
	list-style-type none
#acordeon ul ul li a 
	color white
	text-decoration none
	font-size 11px
	line-height 27px
	display block
	padding 0 15px
#acordeon ul ul
	display none
#acordeon li.active ul
	display block
#acordeon a 
	color white 
	text-decoration none
	font-size 11px
	line-hight 27px
#acordeon h3 .icon-ap1:before
	content: "fa-cube"
#acordeon h3 .icon-ap2:before
	content: "fa-ge"
#acordeon h3 .icon-ap3:before
	content: "fa-git"
#acordeon h3 .icon-ap4:before
	content: "fa-fax"

						
													